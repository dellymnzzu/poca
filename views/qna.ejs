<!doctype html>
<html lang="ko">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css" integrity="sha384-xOolHFLEh07PJGoPkLv1IbcEPTNtaed2xpHsD9ESMhqIYd0nLMwNLD69Npy4HI+N" crossorigin="anonymous">
    <link rel = "stylesheet" href = "../public/stylesheets/searchInerface.css">

    <title>BUTTERFIY</title>
  </head>
  <body>

    <%- include('./nav.html') %>



    
   


    <section class="notice">
      <div class="page-title">
            <div class="container">
                <h3>문의사항</h3>
            </div>
        </div>
    
        <!-- board seach area -->
    <div id="board-search">
      <div class="container">
          <div class="search-window">
              
                  <div class="search-wrap">
                      
                      <input class="form-control"  id = "search-input" placeholder="검색어를 입력해주세요.">
                      <button class="btn btn-dark" id="search">검색</button>
                  </div>
              
          </div>
      </div>
  </div>
       
      <!-- board list area -->
        <div id="board-list">
            <div class="container">
                <table class="board-table">
                    <thead>
                    <tr>
                        <th scope="col" class="th-num">num</th>
                        <th scope="col" class="th-title">title</th>
                        <th scope="col" class="th-date">name</th>
                    </tr>
                    </thead>
                    <tbody>
                    <% for(var i= 0;i<posts.length;i++){ %>
                     <tr>
                       <td>
                         <%= posts[i]._id%>
                         
                        </td>
                        <th>
                          <button data-id="<%= posts[i]._id %>" class="ediy"><%= posts[i].제목%></button> 
                          
                          
                          
                          
                        </th>
                        <td><%= posts[i].작성자%></td>

                        <% } %> 
                    </tr>
    
                    
                  </tbody>
                </table>
              </div>
              <button type="submit" id = "qnaWrite" float="right" class="btn btn-secondary">글쓰기</button>
            </div>

          <div class = pagenationCenter>
            <ul class="pagination">
              <li class="page-item">
                <a class="page-link"  aria-label="Previous">
                  <span aria-hidden="true">&laquo;</span>
                </a>
              </li>
              <li class="page-item">
                <% for(var i= 1;i<6; i++){ %>
                  <a class="page-link-num" data-id = "<%= i * 1 %>" ><%= i * 1 %></a>
                  <% } %></li>
              <li class="page-item">
                <a class="page-link"  aria-label="Next">
                  <span aria-hidden="true">&raquo;</span>
                </a>
              </li>
            </ul>
          </div>
    
    </section>


    <script src="https://code.jquery.com/jquery-3.4.1.js"></script>
    <script type="text/javascript" src="../public/javascript/qna.js"></script>  
    <script type="text/javascript" src="../public/javascript/pagecontrol.js"></script>


    <!-- Optional JavaScript; choose one of the two! -->

    <!-- Option 1: jQuery and Bootstrap Bundle (includes Popper) -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-Fy6S3B9q64WdZWQUiU+q4/2Lc9npb8tCaSX9FK7E8HnRr0Jz8D6OP9dO5Vg3Q9ct" crossorigin="anonymous"></script>


    <!-- Option 2: Separate Popper and Bootstrap JS -->
